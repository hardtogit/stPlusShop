<view class="unipop__ui_panel"><block wx:if="{{opts.shade}}"><view data-event-opts="{{[['tap',[['shadeTaped',['$event']]]]]}}" class="unipop__ui_mask" bindtap="__e"></view></block><view class="unipop__ui_main"><view class="unipop__ui_child" style="{{(opts.style)}}"><block wx:if="{{opts.title}}"><view class="unipop__ui_tit">{{opts.title}}</view></block><block wx:if="{{opts.content}}"><view class="unipop__ui_cnt" style="{{(opts.contentStyle)}}">{{''+opts.content+''}}</view></block><block wx:if="{{opts.btns}}"><view class="unipop__ui_btns"><block wx:for="{{opts.btns}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text data-event-opts="{{[['tap',[['btnTaped',['$0'],[[['opts.btns','',index]]]]]]]}}" class="btn" style="{{(item.style)}}" bindtap="__e">{{item.text}}</text></block></view></block><view class="unipop__ui_btns"><button data-event-opts="{{[['tap',[['cancel',['$event']]]]]}}" bindtap="__e">取消</button><button class="navigator" open-type="getPhoneNumber" bindgetphonenumber="__e" data-event-opts="{{[['getphonenumber',[['getPhoneNumber',['$event']]]]]}}">确定</button></view></view><block wx:if="{{opts.xclose}}"><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="unipop__xclose" bindtap="__e"></view></block></view></view>