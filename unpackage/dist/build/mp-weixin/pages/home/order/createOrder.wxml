<view class="create-order"><navigator class="address-section" url="/pagesA/address/address?source=1"><view class="order-content"><text class="yticon icon-shouhuodizhi"></text><view class="cen"><view class="top"><text class="name">{{addressData.consignee==''||addressData.consignee==null?'点击添加地址':addressData.consignee}}</text><text class="mobile">{{addressData.mobile}}</text></view><block wx:if="{{addressData.consignee}}"><text class="address">{{addressData.address}}</text></block></view><text class="yticon icon-you"></text></view></navigator><block wx:if="{{shopData.hot&&shopData.hot.length!==0}}"><view class="goods-section"><block wx:for="{{shopData.hot}}" wx:for-item="item" wx:for-index="__i0__" wx:key="storeId"><view><view class="g-header"><text class="label">店铺商品</text><text class="name">{{"-"+item.store.name}}</text></view><block wx:for="{{item.list}}" wx:for-item="goods" wx:for-index="__i1__" wx:key="id"><view class="g-item"><view class="top"><image src="{{url+goods.goods.firstImg}}"></image><view class="item-right"><text class="title clamp">{{goods.goods.prodName}}</text><text class="spec">{{goods.goods.subHeading}}</text></view></view><view class="yt-list"><view class="yt-list-cell b-b"><text class="cell-tit clamp">价格</text><text class="cell-tip">{{"￥"+goods.goods.priceEdit+" x "+goods.cpCart.goodsCount}}</text></view></view></view></block><view class="yt-list-cell b-b receve"><text class="cell-tit clamp">收货方式</text><view class="content"><view data-event-opts="{{[['tap',[['changeAcceptType',['$0','zq'],[[['shopData.hot','store.storeId',item.store.storeId,'store.storeId']]]]]]]}}" class="{{['radio','radio',item.acceptType=='zq'&&'active']}}" bindtap="__e"><view class="circle"></view>自取</view><view data-event-opts="{{[['tap',[['changeAcceptType',['$0','dd'],[[['shopData.hot','store.storeId',item.store.storeId,'store.storeId']]]]]]]}}" class="{{['radio','radio',item.acceptType=='dd'&&'active']}}" bindtap="__e"><view class="circle"></view>{{'快递¥'+item.fee.fee+''}}</view></view></view><view class="yt-list-cell  b-b"><text class="cell-tit clamp">备注</text><input class="cell-tip" style="text-align:right;" type="text" placeholder="请填写备注信息" data-event-opts="{{[['input',[['__set_model',['$0','buyerMessage','$event',[]],[[['shopData.hot','store.storeId',item.store.storeId]]]]]]]}}" value="{{item.buyerMessage}}" bindinput="__e"/></view></view></block></view></block><block wx:if="{{shopData.platform&&shopData.platform.length!==0}}"><view class="goods-section"><block wx:for="{{shopData.platform}}" wx:for-item="item" wx:for-index="__i2__" wx:key="storeId"><view><view class="g-header"><text class="label">巨划算商品</text><text class="name">{{"-"+item.store.name}}</text></view><block wx:for="{{item.list}}" wx:for-item="goods" wx:for-index="__i3__" wx:key="id"><view class="g-item"><view class="top"><image src="{{url+goods.goods.firstImg}}"></image><view class="item-right"><text class="title clamp">{{goods.goods.prodName}}</text><text class="spec">{{goods.goods.subHeading}}</text></view></view><view class="yt-list"><view class="yt-list-cell b-b"><text class="cell-tit clamp">价格</text><text class="cell-tip">{{"￥"+goods.goods.priceEdit+" x "+goods.cpCart.goodsCount}}</text></view></view></view></block><view class="yt-list-cell b-b receve"><text class="cell-tit clamp">收货方式</text><view class="content"><block wx:if="{{!item.free}}"><view class="{{['radio','radio','active']}}"><view class="circle"></view>自取</view></block><block wx:if="{{item.free}}"><view class="{{['radio','radio','active']}}"><view class="circle"></view>快递免邮</view></block></view></view><view class="yt-list-cell  b-b"><text class="cell-tit clamp">备注</text><input class="cell-tip" style="text-align:right;" type="text" placeholder="请填写备注信息" data-event-opts="{{[['input',[['__set_model',['$0','buyerMessage','$event',[]],[[['shopData.platform','store.storeId',item.store.storeId]]]]]]]}}" value="{{item.buyerMessage}}" bindinput="__e"/></view></view></block></view></block><block wx:if="{{shopData.coupon&&shopData.coupon.length!==0}}"><view class="goods-section"><block wx:for="{{shopData.coupon}}" wx:for-item="item" wx:for-index="__i4__" wx:key="storeId"><view><view class="g-header"><text class="label">商圈券</text><text class="name">{{"-"+item.store.name}}</text></view><block wx:for="{{item.list}}" wx:for-item="goods" wx:for-index="__i5__" wx:key="id"><view class="g-item"><view class="top"><image src="{{url+goods.goods.imgPath}}"></image><view class="item-right"><text class="title clamp">{{goods.goods.name}}</text><text class="spec">{{goods.goods.instro}}</text></view></view><view class="yt-list"><view class="yt-list-cell b-b"><text class="cell-tit clamp">价格</text><text class="cell-tip">{{"￥"+goods.goods.value/100+" x "+goods.cpCart.goodsCount}}</text></view></view></view></block><view class="yt-list-cell  b-b"><text class="cell-tit clamp">备注</text><input class="cell-tip" style="text-align:right;" type="text" placeholder="请填写备注信息" data-event-opts="{{[['input',[['__set_model',['$0','buyerMessage','$event',[]],[[['shopData.coupon','store.storeId',item.store.storeId]]]]]]]}}" value="{{item.buyerMessage}}" bindinput="__e"/></view></view></block></view></block><view class="footer"><view class="price-content"><text>实付款</text><text class="price-tip">￥</text><text class="price">{{total}}</text></view><text data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="submit" bindtap="__e">提交订单</text></view></view>