<view class="content"><view class="row b-b"><text class="tit">联系人</text><input class="input" type="text" placeholder="收货人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','consignee','$event',[]],['addressData']]]]]}}" value="{{addressData.consignee}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号</text><input class="input" type="number" placeholder="收货人手机号码" placeholder-class="placeholder" maxlength="11" data-event-opts="{{[['input',[['__set_model',['$0','mobile','$event',[]],['addressData']]]]]}}" value="{{addressData.mobile}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['showPicker',['$event']]]]]}}" class="row b-b" bindtap="__e"><text class="tit">地区</text><view class="li"><view class="input">{{''+pickerCity+''}}</view></view></view><mpvue-picker class="vue-ref" vue-id="1" mode="{{mode}}" deepLength="{{deepLength}}" pickerValueDefault="{{pickerValueDefault}}" themeColor="{{themeColor}}" pickerValueArray="{{pickerValueArray}}" data-ref="mpvuePicker" data-event-opts="{{[['^onChange',[['onChange']]],['^onConfirm',[['onConfirm']]],['^onCancel',[['onCancel']]]]}}" bind:onChange="__e" bind:onConfirm="__e" bind:onCancel="__e" bind:__l="__l"></mpvue-picker><view class="row b-b"><text class="tit">详细地址</text><input class="input" type="text" placeholder="详细地址" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','addrDetail','$event',[]],['addressData']]]]]}}" value="{{addressData.addrDetail}}" bindinput="__e"/></view><view class="row default-row"><text class="tit">设为默认</text><switch checked="{{isDefault}}" color="#004da0" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button></view>