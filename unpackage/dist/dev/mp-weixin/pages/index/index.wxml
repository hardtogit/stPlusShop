<view class="container"><uni-popup class="userPop vue-ref" vue-id="1" type="center" data-ref="popup" bind:__l="__l" vue-slots="{{['default']}}"><view><uni-pop class="vue-ref" bind:changes="__e" vue-id="{{('2')+','+('1')}}" data-ref="uniPop" data-event-opts="{{[['^changes',[['childClick']]]]}}" bind:__l="__l"></uni-pop></view></uni-popup><block wx:if="{{posterImgLength>0}}"><view class="carousel-section"><view class="titleNview-background" style="{{'background-color:'+(titleNViewBackground)+';'}}"></view><swiper class="carousel" circular data-event-opts="{{[['change',[['imgChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{posterImgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item class="carousel-item"><image src="{{url+item}}"></image></swiper-item></block></swiper><view class="swiper-dots"><text class="num">{{posterImgCurrent+1}}</text><text class="sign">/</text><text class="num">{{posterImgLength}}</text></view></view></block><view class="guess-section"><block wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['goodsList','',index]]]]]]]}}" class="guess-item" catchtap="__e"><view class="image-wrapper"><image src="{{url+item.resource.samllCoverImg}}" mode="aspectFill"></image></view><text class="title clamp">{{item.resource.name}}</text><view class="price-box"><block wx:if="{{item.goods.isFaceToFace!=1}}"><view class="price-box"><text class="price">{{item.goods.priceEdit}}</text></view></block><block wx:if="{{item.goods.isFaceToFace==1||item.goods.isFaceToFace=='1'}}"><view class="price-box"><text class="price">面议</text></view></block><text>{{"余"+item.goods.stock+"件"}}</text><text data-event-opts="{{[['tap',[['addCart',['$0'],[[['goodsList','',index,'goods']]]]]]]}}" class="yticon icon-gouwuche" catchtap="__e"></text></view></view></block></view><uni-load-more vue-id="3" status="{{loadingType}}" bind:__l="__l"></uni-load-more></view>