/*仿照头部*/
.customHead{ height: 100rpx; width: 100%; position: fixed; top: 0; left: 0; z-index: 50; background: #fff; padding: 0 6rpx; padding-left: 60rpx;}
.customHead .name{ line-height: 100rpx;font-size: 34rpx; color: #000; margin-left: 24rpx; font-weight: bold !important;}
.customHead .rightLabel{ position: absolute; padding: 0 40rpx; height:60rpx; line-height: 60rpx; background: #8cd8ff; color: #fff; border-radius: 30px; right: 24rpx; top: 50%;-webkit-transform: translateY(-50%);transform: translateY(-50%);}
.customHead .rightLabel .label{ font-size: 26rpx;}
.customHead .rightLabel .iconfont{ font-size: 30rpx; margin-right: 10rpx;}
.customHead .rightLabel .badge{position: absolute; top: 0; right: -5rpx; height: 30rpx; line-height: 30rpx; padding: 0 10rpx; border-radius: 30px; border-bottom-left-radius: 0; background: #549ff2; font-size: 24rpx; color: #fff;}
.customHead .leftReturn{position: absolute; left: 0; top: 50%; color:#000; font-size: 72rpx; -webkit-transform: translateY(-50%);transform: translateY(-50%);}
/*公共样式*/
.indexItem:after,.formListAfter:after,.memberDel:after,.indexTop .labelAmend:after,.homePageBottom .arrow:after,.shopItem .handle .visual:after,.payMethod .list:after,.LabelAddress:after,.confirmAddress:after,.businessMoreList:after,.businessCardIco .icoBtn:after{font-family: "iconfont" !important; font-size: 16px;font-style: normal;-webkit-font-smoothing: antialiased;-moz-osx-font-smoothing: grayscale;}
.buttonFixed{position: fixed; bottom: 0; left: 0;width: 100%; z-index: 100; width: 100%; background: #fff;}
.buttonBigWidth{width:90%; margin:2% 5%; background: #549ef3; color: #fff; border-radius: 5px; height: 90rpx; line-height: 90rpx; text-align: center; font-size: 34rpx;}
.buttonBigWidth:active{background: #549ff2;}
.ellipsis{
	text-overflow: ellipsis;white-space: nowrap;overflow: hidden;
}
/*表单*/
.formList{width: 100%; display: block; background: #fff; overflow: hidden;}
.formListCon{width: 100%; display: block; position: relative; overflow: hidden; padding: 24rpx 20rpx; border-bottom: 1px solid #f5f5f5;}
.formListCon:last-child{ border: none;}
.formListCon .name{ float: left; font-size: 28rpx; height: 54rpx !important; line-height: 56rpx !important; color: #333;}
.formListCon .name.nameBlock{width: 100%;}
.formListCon .right{ -webkit-box-flex: 1; -webkit-flex: 1; flex: 1; display: block; float: right; text-align: right;}
.formListCon .inInput,.formListCon .inCon{width: 100%; height: 54rpx !important; line-height: 56rpx !important; overflow: hidden; font-size: 30rpx; color: #888;}
.formListCon .placeholder{ color: #ccc;font-size: 28rpx;}
.formListCon .inCon{text-overflow: ellipsis;white-space: nowrap;overflow: hidden;}
.formListCon textarea.inInput{ height: 200rpx !important;}
.formListCon .right.rightBlock{ width: 100%; display: block; margin-top: 10rpx;}
.formListCon .right.rightBlock textarea.inInput{ text-align: left;}
.formListCon .right.rightBlock .inCon{ text-align: left; height: auto !important;white-space: inherit;}
.formListCon .formRadio{ display: -webkit-box; display: -webkit-flex; display: flex; -webkit-box-orient: horizontal; -webkit-box-direction: normal; -webkit-flex-direction: row; flex-direction: row;-webkit-box-align:center;-webkit-align-items:center;align-items:center; -webkit-box-pack: end; -webkit-justify-content: flex-end; justify-content: flex-end;}
.formListCon .formRadioList{ margin-left:30rpx; font-size: 31rpx; color: #666;}
uni-radio .uni-radio-input{width: 36rpx; height: 36rpx; float: right;}
uni-radio .uni-radio-input.uni-radio-input-checked:before{font-size: 14px;}
.formRadioList .radioIco{ vertical-align: middle; display: inline-block; margin-right: 6rpx;}
.formRadioList .radioName{ vertical-align: middle;display: inline-block;font-size: 31rpx;
}
uni-radio .uni-radio-input-checked{ background: #549ff2!important; border: 1px solid #549ff2!important;}
uni-checkbox .uni-checkbox-input{width: 38rpx; height: 38rpx;}
uni-checkbox .uni-checkbox-input.uni-checkbox-input-checked:before{ font-size: 38rpx;}
.formListCon .upPicture .sunsin_picture_list{ padding: 0 !important;}
.formListCon .sunsin_picture_item{width: 156rpx !important; height: 156rpx !important;}
.formRadioTwo  .formRadioList{width: 50%;display: inline-block;float: left;margin: 10rpx 0;text-align: left; font-size: 30rpx;}
.formRadioTwo  .formRadioList .radioName{font-size: 28rpx; color: #444;}
.formList .formListAfter {padding-right:66rpx;
}
.formListAfter:after{content: '\E6FF';position: absolute; right: 10rpx; top: 50%;-webkit-transform: translateY(-50%);transform: translateY(-50%); color: #ccc; font-size:60rpx;}
.formListCon .inConSmall,.formRadioList .labelTip{ font-size: 26rpx !important; color: #999 !important;}
.formRadioList .labelTip{ margin-left: 20rpx;}
.formListCon .rightLeft{ width: 100%; text-align: left; float: left;}
.formListPhoto .photo{ width: 120rpx; height: 120rpx; border-radius: 100%; overflow: hidden; float: right;}
.formListPhoto .photo image{width: 100%; height: 100%;}
.formListPhoto.formListCon .name{ height: 120rpx !important; line-height: 120rpx !important;}
.upOneImg{ width: 180rpx; height: 180rpx; border-radius: 5px; overflow: hidden; margin-top: 25rpx;}
.upOneImg image{width: 100%; height: 100%;}
uni-radio-group uni-label.formRadioListText { padding-right: 0;}
.formRadioListText uni-radio .uni-radio-input{ margin-right: 0;}
.upMoreImg{ width: 158rpx; height: 158rpx; float: left; position: relative; margin: 20rpx 20rpx 0 0;}
.upMoreImg .photo{width: 100%; height: 100%; border-radius: 5px; overflow: hidden;}
.upMoreImg .photo image{width: 100%; height: 100%;}
.upMoreImg .dodel{position: absolute; right: -10rpx; top:  -15rpx; width: 80rpx; height: 80rpx;}
.upMoreImg .dodel image{ width: 40rpx; height: 40rpx; float: right;}
.upMoreImgCon{ margin-right:-20rpx;}
/*弹出层*/
.uni-mask{ top:0 !important; bottom: 0 !important;}
/* .uni-popup{width: 80%;} */
.tipPopupCon{width: 100%;}
.tipTitle{text-align: center;font-weight: 700;font-size: 32rpx; color: #333; margin-bottom: 20rpx;}
.tipContent{padding:20rpx;font-size:28rpx;color: #666; text-align: center;}
.tipGroup{ width: 100%; display: -webkit-box; display: -webkit-flex; display: flex; -webkit-box-align: center; -webkit-align-items: center; align-items: center;}
.tipGroupBtn{width: 50%; text-align: center; padding-top: 30rpx; color: #000;}
/*文本编辑器*/
.EditorHandle{width: 100%; display: block; overflow: hidden; border: 1px solid #f5f5f5; margin-top: 20rpx;}
.EditorHandle .EditorText,.EditorHandle .EditorAdd,.EditorHandle .EditorImage{ width: 33.3%; float: left; display: -webkit-box; display: -webkit-flex; display: flex; -webkit-box-orient: horizontal; -webkit-box-direction: normal; -webkit-flex-direction: row; flex-direction: row; -webkit-box-align: center; -webkit-align-items: center; align-items: center; -webkit-box-pack: center; -webkit-justify-content: center; justify-content: center; padding:30rpx 0;}
.EditorHandle image{width: 70rpx; height: 70rpx;
}
.EditorContent,.EditorTextarea,.EditorImages{width: 100%; display: block; overflow: hidden;}
.EditorTextarea,.EditorImages{border: 1px solid #f5f5f5; margin-top: 20rpx; position: relative;}
.EditorTextarea{padding: 20rpx 30rpx;
}
.EditorTextarea .textarea{ height:240rpx; line-height:45rpx; font-size: 28rpx; color: #666;width: 100%;}
.EditorTextarea .textareaDel{ float: right; font-size: 26rpx; color: #549ef3; padding-left: 30rpx; margin-top: 10rpx;}
.EditorImages .photo{width: 100%; height: 340rpx; overflow: hidden;}
.EditorImages .photo image{width: 100%;}
.EditorImages .photoLayer{position: absolute; bottom: 0; left: 0; width: 100%; z-index: 20; background: rgba(0,0,0,0.6);}
.EditorImages .photoLayer .EditorLabel{ float: right;font-size: 26rpx; color: #fff; padding: 14rpx 30rpx;}
.uni-switch-wrapper{ margin-left: 20rpx;}
uni-switch .uni-switch-input{ height:46rpx; margin-top: -10rpx; width: 84rpx;}
uni-switch .uni-switch-input:after, uni-switch .uni-switch-input:before{ height:42rpx;}
uni-switch .uni-switch-input:after{ height: 42rpx; width: 42rpx;}
uni-switch .uni-switch-input:before{width: 80rpx;}
